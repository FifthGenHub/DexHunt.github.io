{
  "extends": [
    "development"
  ],
  "hints": {
    "apple-touch-icons": "off",
    "no-inline-styles": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select"
        ]
      }
    ],
    "disown-opener": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 137",
    "not firefox <= 138",
    "not opera <= 117"
  ]
}